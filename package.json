{
  "name": "html-dnd",
  "version": "1.0.1",
  "description": "HTML Drag and Drop Simulator for E2E testing",
  "keywords": [
    "drag",
    "drop",
    "dnd",
    "test",
    "webdriver"
  ],
  "main": "lib/index.js",
  "devDependencies": {
    "chai": "^3.3.0",
    "chromedriver": "^2.19.0",
    "eslint": "^1.6.0",
    "jscs": "^2.2.1",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "selenium-webdriver": "^2.47.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint src lib tests && jscs src lib tests",
    "local-test": "mocha tests/unit/node && karma start karma.conf.js && mocha tests/e2e/html_dnd.js",
    "test": "mocha tests/unit/node && karma start karma.conf-ci.js && mocha --opts tests/e2e/mocha.opts tests/e2e"
  },
  "author": "Kuniwak",
  "license": "MIT",
  "dependencies": {
    "multiline": "^1.0.2"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kuniwak/html-dnd.git"
  },
  "bugs": {
    "url": "https://github.com/Kuniwak/html-dnd/issues"
  },
  "homepage": "https://github.com/Kuniwak/html-dnd#readme"
}
