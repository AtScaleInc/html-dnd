{
  "name": "html-dnd",
  "version": "1.0.1",
  "description": "HTML Drag and Drop Simulator for E2E testing",
  "keywords": [
    "drag",
    "drop",
    "dnd",
    "test",
    "webdriver"
  ],
  "main": "lib/index.js",
  "devDependencies": {
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "dotenv": "^2.0.0",
    "eslint": "^2.7.0",
    "jscs": "^2.11.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sauce-launcher": "^0.3.1",
    "lodash": "^4.10.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.8.0",
    "selenium-webdriver": "^2.53.1"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "npm-run-all lint:*",
    "lint:style": "jscs src lib test",
    "lint:problem": "eslint src lib test",
    "unit:node": "mocha test/unit/node/**/*.js",
    "unit:browser": "karma start karma.conf.js",
    "e2e:selenium-webdriver": "mocha test/e2e/selenium-webdriver/**/*.js",
    "test": "npm-run-all unit:* e2e:*"
  },
  "author": "Kuniwak",
  "license": "MIT",
  "dependencies": {
    "multiline": "^1.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kuniwak/html-dnd.git"
  },
  "bugs": {
    "url": "https://github.com/Kuniwak/html-dnd/issues"
  },
  "homepage": "https://github.com/Kuniwak/html-dnd#readme"
}
